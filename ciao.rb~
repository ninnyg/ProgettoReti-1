class Persona
   @nome
    @cognome 

    def initialize (nome,cognome)
        @nome = nome
        @cognome = cognome
    end
    def to_s()
	return @nome+@cognome
    end
end
class Ufficio
    @indirizzo
    @metratura 

    def initialize (indirizzo, metratura)
        @indirizzo = indirizzo
        @metratura = metratura
    end
    def to_s()
	return @indirizzo+@metratura
    end
end
p=Persona.new("pippo","pippa")
i=Ufficio.new("sia",500)
puts p
puts i
